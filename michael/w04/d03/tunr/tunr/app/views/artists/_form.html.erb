

<%= form_for @artist do |f| %>

<div class="form-group">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
</div>
<div class="form-group">
    <%= f.label :hometown %><br>
    <%= f.text_field :hometown %>
</div>
<div class="form-group">
    <%= f.label :bio %><br>
    <%= f.text_field :bio %>
</div>
<div class="form-group">
    <%= f.label :years_active %><br>
    <%= f.text_field :years_active %>
</div>
<div class="form-group">
    <%= f.label :label %><br>
    <%= f.text_field :label %>
</div>
<div class="form-group">
    <%= f.submit%>
</div>

<% end %>

