<%= form_for @book do |f| %>

<div class="form-group">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
<div>

  <div class="form-group">
    <%= f.label :cuisine %><br>
    <%= f.text_field :cuisine %>
<div>

<div class="form-group">
    <%= f.label :chef %><br>
    <%= f.text_field :chef %>
<div>

  <div class="form-group">
    <%= f.label :image %><br>
    <%= f.url_field :image %>
<div>
    <div class="form-group">
    <%= f.submit%>
<div>

<% end %>

